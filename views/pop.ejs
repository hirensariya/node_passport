<!doctype html>
<html lang="en">
    <head>
        <%- include('./partials/head.ejs') %> 
    </head>
    <style>
   
  </style>
    <body>

        <div class="container-fluid">
            <%- include('./partials/nav.ejs') %> 

            <form action="/set" method="POST">
                <div class="down row">
                  <div class="col-md-4">
                    <div class="form-group">
                        <label for="applying-for">Applying for</label>
                        <select class="form-control" id="applying-for" name="application_for">
                            <option value="fresh">New / Fresh Passport</option>
                            <option value="reissue">Reissue Passport</option>
                        </select>
                    </div>
                </div>
                <div class="col-md-4">
                  <div class="form-group">
                      <label for="type-of-application">Type of Application</label>
                      <select class="form-control" id="type-of-application" name="application_type">
                          <option value="normal">Normal</option>
                          <option value="tatkaal">Tatkaal</option>
                      </select>
                  </div>
              </div>
          
              <div class="col-md-4">
                  <div class="form-group">
                      <label for="type-of-passport-booklet">Type of Passport
                          Booklet</label>
                      <select class="form-control" id="type-of-passport-booklet" name="application_booklet">
                          <option value="36-pages">36 Pages</option>
                          <option value="60-pages">60 Pages</option>
                      </select>
                  </div>
              </div>
                  <div class="form-group col-xl-4 col-md-5 col-sm-5">
                    <label for="exampleInputEmail1">Name</label>
                    <input type="text" class="form-control" id="exampleInputEmail1"
                      aria-describedby="emailHelp" name="application_name">
                  </div>
                  <div class="form-group col-xl-4 col-md-5 col-sm-5">
                    <label for="exampleInputEmail1">phone</label>
                    <input type="text" class="form-control" id="exampleInputEmail1"
                      aria-describedby="emailHelp" name="application_phone">
                  </div>
                  <div class="form-group col-xl-4 col-md-5 col-sm-5">
                    <label for="exampleInputEmail1">age</label>
                    <input type="text" class="form-control" id="exampleInputEmail1"
                      aria-describedby="emailHelp" name="application_age">
                  </div>
                  <div class="form-group col-xl-4 col-md-5 col-sm-5">
                    <label for="exampleInputEmail1">promo</label>
                    <input type="text" class="form-control" id="exampleInputEmail1"
                      aria-describedby="emailHelp" name="promo">
                  </div>
                </div>
                <button type="submit" class="btn btn-primary sub">Submit</button>
              </form>
              
            <%- include('./partials/footer.ejs') %> 
        </div>





        <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"
            integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj"
            crossorigin="anonymous"></script>
        <script
            src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-ho+j7jyWK8fNQe+A12Hb8AhRq26LrZ/JpcUGGOn+Y7RsweNrtN/tE3MoK7ZeZDyx"
            crossorigin="anonymous"></script>


    </body>
</html>